    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["validation.js"],
    "rules": {
        "quotes": ["warn", "double"]
    }
}
