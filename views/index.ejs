<%- include('partials/index-header') -%>
<main class="container-fluid d-flex align-items-center mx-0 px-0" style="height: 100vh; margin-top: -1rem;">
		
    <div id="mainCarousel" class="carousel slide bg-inverse w-100 ms-auto me-auto mt-5" data-bs-ride="carousel">
        <ol class="carousel-indicators">
            <li data-bs-target="#mainCarousel" data-bs-slide-to="0" class="active"></li>
            <li data-bs-target="#mainCarousel" data-bs-slide-to="1"></li>
            <li data-bs-target="#mainCarousel" data-bs-slide-to="2"></li>
        </ol>
        <div class="carousel-inner bg-custom" role="listbox">
            <div class="carousel-item active">
                <img class="carousel-image" src="https://www.mainegardens.org/wp-content/uploads/2021/06/sm-CMBG-DSC-1877-1024x683.jpg" alt="First slide">
                <div class="carousel-caption">
                    <h2 class="text-capitalize title-font fw-bold">dream & plan green spaces</h2>
                    <div class="row justify-content-around mt-5">
                        <div class="col"><a class="btn btn-primary" href="/login" role="button">Login</a>
                            <a class="btn btn-primary" href="/signup" role="button">Signup</a></div>
                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <img class="carousel-image" src="https://i.insider.com/5b2a856b5e48ec14098b4647?width=1200" alt="Second slide">
                <div class="carousel-caption">
                    <h2 class="text-capitalize title-font fw-bold">catalog & track plants</h2>
                    <p>This is the second slide.</p>
                </div>
            </div>
            <div class="carousel-item">
                <img class="carousel-image" src="https://i.guim.co.uk/img/media/faa9a7312203b9824ff6856472d4f41c1747251e/6_0_3481_2090/master/3481.jpg?width=445&quality=45&auto=format&fit=max&dpr=2&s=7b0605ac95038c86ac6f710d0ec6bc37" alt="Third slide">
                <div class="carousel-caption">
                    <h2 class="text-capitalize title-font fw-bold">catalog your plants</h2>
                    <p>This is the third slide.</p>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#mainCarousel" role="button" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#mainCarousel" role="button" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </a>
    </div>
    
</main>
<div class="container">
    
    
</div>
    

